"use client";

import { useEffect, useState, useRef, useCallback } from "react";
import { listen } from "@tauri-apps/api/event";
import { invoke } from "@tauri-apps/api/core";
import { checkForUpdate } from "./utils/checkForUpdate";

export default function Page() {
  const [logLines, setLogLines] = useState([]);
  const [updateMessage, setUpdateMessage] = useState("");
  const [loading, setLoading] = useState(false);
  const [completed, setCompleted] = useState(false);
  const [shouldAutoScroll, setShouldAutoScroll] = useState(true);
  const logContainerRef = useRef(null);

  // „É¶„Éº„Ç∂„Éº„ÅÆ„Çπ„ÇØ„É≠„Éº„É´Êìç‰Ωú„ÇíÊ§úÁü•„Åô„Çã„Ç§„Éô„É≥„Éà„Éè„É≥„Éâ„É©
  const handleScroll = () => {
    if (!logContainerRef.current) return;
    
    const { scrollTop, scrollHeight, clientHeight } = logContainerRef.current;
    // „Çπ„ÇØ„É≠„Éº„É´‰ΩçÁΩÆ„ÅåÊúÄ‰∏ãÈÉ®„Åã„Çâ20px‰ª•ÂÜÖ„Å™„ÇâËá™Âãï„Çπ„ÇØ„É≠„Éº„É´„ÇíÊúâÂäπÂåñ
    const isNearBottom = scrollHeight - scrollTop - clientHeight < 20;
    
    setShouldAutoScroll(isNearBottom);
  };

  // „Çπ„ÇØ„É≠„Éº„É´„ÇíÊúÄ‰∏ãÈÉ®„Å´ÁßªÂãï„Åô„ÇãÈñ¢Êï∞ÔºàuseCallback„Åß„É°„É¢ÂåñÔºâ
  const scrollToBottom = useCallback(() => {
    if (logContainerRef.current && shouldAutoScroll) {
      // Âº∑Âà∂ÁöÑ„Å´„Çπ„ÇØ„É≠„Éº„É´„ÇíÂÆüË°å
      const container = logContainerRef.current;
      container.scrollTop = container.scrollHeight;
    }
  }, [shouldAutoScroll]);

  // „É≠„Ç∞„ÅåÊõ¥Êñ∞„Åï„Çå„Åü„Å®„Åç„Å´Âº∑Âà∂ÁöÑ„Å´„Çπ„ÇØ„É≠„Éº„É´„ÇíÂÆüË°å
  useEffect(() => {
    if (logLines.length > 0 && shouldAutoScroll) {
      // Ë§áÊï∞„ÅÆÊñπÊ≥ï„ÇíÁµÑ„ÅøÂêà„Çè„Åõ„Å¶Á¢∫ÂÆü„Å´„Çπ„ÇØ„É≠„Éº„É´„ÇíÂÆüË°å
      // 1. Âç≥ÊôÇÂÆüË°å
      scrollToBottom();
      
      // 2. Ê¨°„ÅÆ„Éï„É¨„Éº„É†„ÅßÂÆüË°å
      requestAnimationFrame(scrollToBottom);
      
      // 3. Â∞ë„ÅóÈÅÖÂª∂„Åï„Åõ„Å¶ÂÆüË°åÔºà„É¨„É≥„ÉÄ„É™„É≥„Ç∞ÂÆå‰∫ÜÂæåÔºâ
      setTimeout(scrollToBottom, 50);
    }
  }, [logLines, scrollToBottom]);

  // „Çπ„ÇØ„É≠„Éº„É´„Ç§„Éô„É≥„Éà„É™„Çπ„Éä„Éº„ÅÆÁôªÈå≤„Å®Ëß£Èô§
  useEffect(() => {
    const logContainer = logContainerRef.current;
    if (logContainer) {
      logContainer.addEventListener('scroll', handleScroll);
      return () => logContainer.removeEventListener('scroll', handleScroll);
    }
  }, []);

  useEffect(() => {
    let isUnmounted = false;
    let unlistenCallback = null;

    const startPython = async () => {
      const script = "main.py";
      const params = {
        mode: "csv",
        folder: "./data",
        plant_name: "Â∑•Â†¥B",
      };

      try {
        // „É™„Çπ„Éä„Éº„ÇíÁôªÈå≤Ôºà„É≠„Ç∞Âèó‰ø°Ôºâ- „Éû„Ç¶„É≥„ÉàÁä∂ÊÖã„ÇíÁ¢∫Ë™ç
        unlistenCallback = await listen("python-log", (event) => {
          // „Ç≥„É≥„Éù„Éº„Éç„É≥„Éà„Åå„Ç¢„É≥„Éû„Ç¶„É≥„Éà„Åï„Çå„Å¶„ÅÑ„Åü„Çâ‰Ωï„ÇÇ„Åó„Å™„ÅÑ
          if (isUnmounted) return;
          
          const line = event.payload;
          
          // ÈáçË§á„ÇíÈò≤„Åê„Åü„ÇÅ„ÅÆ„Éï„Ç£„É´„Çø„É™„É≥„Ç∞
          // Âêå„ÅòË°å„ÅåÁü≠ÊôÇÈñì„Å´Ë§áÊï∞ÂõûÊù•„ÅüÂ†¥Âêà„ÅØÁÑ°Ë¶ñ„Åô„Çã
          setLogLines((prev) => {
            // Áõ¥Ëøë„ÅÆË°å„Å®Âêå„ÅòÂÜÖÂÆπ„Å™„ÇâËøΩÂä†„Åó„Å™„ÅÑ
            if (prev.length > 0 && prev[prev.length - 1] === line) {
              return prev;
            }
            
            // Êñ∞„Åó„ÅÑ„É≠„Ç∞„ÇíËøΩÂä†„Åó„ÅüÂæå„ÄÅÊ¨°„ÅÆ„Éï„É¨„Éº„É†„Åß„Çπ„ÇØ„É≠„Éº„É´„ÇíÂÆüË°å
            requestAnimationFrame(() => {
              if (logContainerRef.current && shouldAutoScroll) {
                logContainerRef.current.scrollTop = logContainerRef.current.scrollHeight;
              }
            });
            
            return [...prev, line];
          });

          // ‰ªªÊÑèÔºöÁµÇ‰∫Ü„Å£„ÅΩ„ÅÑ„ÉØ„Éº„Éâ„ÅåÊù•„Åü„Çâcompleted„Å´„Åô„Çã
          if (line.includes("CSVÂá¶ÁêÜÂÆå‰∫Ü") || line.includes("ÂÖ®Âá¶ÁêÜÂÆå‰∫Ü")) {
            setCompleted(true);
          }
        });

        // Python„ÅÆÂÆüË°å„Çπ„Çø„Éº„ÉàÔºàstreamingÁâàÔºâ
        if (!isUnmounted) {
          await invoke("run_python_script_streaming", {
            script,
            param: JSON.stringify(params),
          });
        }
      } catch (error) {
        console.error("PythonÂÆüË°å„Ç®„É©„Éº:", error);
      }
    };

    startPython();

    // „ÇØ„É™„Éº„É≥„Ç¢„ÉÉ„ÉóÈñ¢Êï∞
    return () => {
      isUnmounted = true;
      if (unlistenCallback) {
        unlistenCallback();
      }
    };
  }, []);

  const handleUpdateCheck = async () => {
    setLoading(true);
    const msg = await checkForUpdate();
    setUpdateMessage(msg);
    setLoading(false);
  };

  return (
    <div className="p-4 space-y-4">
      <div>
        <h2 className="text-lg font-bold">Python ÂÆüË°å„É≠„Ç∞</h2>
        <pre 
          ref={logContainerRef}
          className="mt-2 p-2 bg-gray-100 border rounded h-80 overflow-auto whitespace-pre-wrap"
          style={{ scrollBehavior: 'smooth' }}
          onScroll={handleScroll}
        >
          {logLines.map((line, idx) => (
            <div key={idx}>{line}</div>
          ))}
        </pre>
        {completed && (
          <p className="text-green-600 mt-2 font-medium">‚úÖ Âá¶ÁêÜ„ÅåÂÆå‰∫Ü„Åó„Åæ„Åó„ÅüÔºÅ</p>
        )}
      </div>

      <div className="p-4 border rounded">
        <h2 className="text-lg font-bold mb-2">„Ç¢„ÉÉ„Éó„Éá„Éº„ÉàÁ¢∫Ë™ç</h2>
        <button
          onClick={handleUpdateCheck}
          disabled={loading}
          className="px-4 py-2 bg-green-600 text-white rounded hover:bg-green-700 disabled:bg-gray-400"
        >
          {loading ? "Á¢∫Ë™ç‰∏≠..." : "„Ç¢„ÉÉ„Éó„Éá„Éº„Éà„ÇíÁ¢∫Ë™ç„Åô„Çã"}
        </button>
        {updateMessage && <p className="mt-4">{updateMessage}</p>}
      </div>
    </div>
  );
}
